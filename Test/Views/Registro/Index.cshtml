@using PagedList;
@using PagedList.Mvc;
@model IPagedList<Test.Models.Evento>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Registrar
        </h1>
        <ol class="breadcrumb">
            <li>
                <i class="fa fa-dashboard"></i><a href="#">Panel Administracion</a>
            </li>
            <li class="active">
                <i class="fa fa-user"></i> Registro
            </li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="row">
            @foreach(var item in Model){
                <div class="panel panel-default col-lg-4">
                    <div class="panel-heading">
                        <h3></h3>
                    </div>
                    <div class="panel-body">
                        <p></p>
                    </div>
                </div>
            }
        </div>
    </div>
</div>